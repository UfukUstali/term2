{
  "copy": {
    "key": "ctrl+c"
  },
  "paste": {
    "key": "ctrl+v, command+v"
  },
  "cursorLeft": [
    {
      "chord": ["ctrl", "h"]
    },
    {
      "key": "h",
      "scope": "cursorAlreadyMoving"
    }
  ],
  "cursorRight": [
    {
      "chord": ["ctrl, command", "l"]
    },
    {
      "key": "l",
      "scope": "cursorAlreadyMoving"
    }
  ]
}
